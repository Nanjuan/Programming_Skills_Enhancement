<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <title>Nestor</title>
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>


  </head>
  <body>
      <script>
            $(document).ready(function(){
                $.getJSON("http://localhost:8001/api/task", function(results){
                    $.each(results.data, function(i, task){
                        var btn = document.createElement("li");   // Create a <button> element
                            btn.innerHTML = task.title;
                        $("#myPage").append(btn);
                    });
                });
            });
      </script>


      <div id="myPage"></div>
</body>
</html>